 
<h1>Products view</h1>

<table class="table">
    <thead>
      <tr>
        <th scope="col">Title</th>
        <th scope="col">Price</th>
        <th scope="col">Img</th>
      </tr>
    </thead>
    <tbody>

<% products.forEach(function(product) { %>
    
    <tr>
        <th scope="row"><%=product.title%></th>
        <td><%=product.price%></td>
        <td><img style="width: 5rem;" src=<%=product.thumbnail%> alt=""></td>
    </tr>
<% }); %> 
</tbody>
</table>

